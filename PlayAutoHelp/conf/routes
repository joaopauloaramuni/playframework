# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET              /                  controllers.Application.index()

# Pages
GET              /admin	            controllers.Application.pagAdmin()
GET              /painel            controllers.Application.pagPainel()
GET              /atend             controllers.Application.pagAtend()
GET              /func              controllers.Application.pagFunc()
GET              /cliente           controllers.Application.pagCliente()
GET              /veiculo           controllers.Application.pagVeiculo()
GET              /servico           controllers.Application.pagServico()
GET              /peca              controllers.Application.pagPeca()
GET              /relatorios        controllers.Application.pagRel()
GET              /email             controllers.Application.pagEmail()
GET              /serv              controllers.Application.pagServ()
GET              /pec               controllers.Application.pagPec()
GET              /orc               controllers.Application.pagOrc()
GET              /quem              controllers.Application.pagQuem()
GET              /contato	        controllers.Application.pagContato()
GET              /mec	       		controllers.Application.pagMec()
GET              /manu	         	controllers.Application.pagManu()
GET              /pneu	            controllers.Application.pagPneu()
GET              /prev	            controllers.Application.pagPrev()
GET              /user	            controllers.Application.pagUser()

GET              /logout            controllers.Application.logout()

# Action

POST             /                  controllers.Application.index()
POST             /admin             controllers.Application.action_admin()
POST             /consulta          controllers.Application.action_consultar()
POST             /email             controllers.Application.action_sendEmail()
POST             /func              controllers.Application.action_func()
POST             /cliente           controllers.Application.action_cliente()
POST             /peca              controllers.Application.action_peca()
POST             /user              controllers.Application.action_user()
POST             /servico           controllers.Application.action_serv()
POST             /veiculo           controllers.Application.action_veic()
POST             /atend             controllers.Application.action_atend()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
